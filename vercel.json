{
	"rewrites": [{ "source": "/(.*)", "destination": "/" }],
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Content-Security-Policy",
					"value": "default-src 'self'; script-src 'self' https://www.gstatic.com https://www.googleapis.com https://firebase.google.com"
				}
			]
		}
	]
}
